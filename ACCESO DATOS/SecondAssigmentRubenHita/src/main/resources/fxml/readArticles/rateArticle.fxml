   <children>
      <TableView fx:id="readArticlesTable" layoutX="525.0" layoutY="81.0" prefHeight="337.0" prefWidth="393.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="127.0" text="id" />
          <TableColumn fx:id="idArticleColumn" prefWidth="117.0" text="idArticle" />
            <TableColumn fx:id="rankingColumn" prefWidth="148.0" text="ranking" />
        </columns>
      </TableView>
      <TextField fx:id="ratingField" layoutX="38.0" layoutY="81.0" promptText="rating" />
      <Button layoutX="38.0" layoutY="120.0" mnemonicParsing="false" onAction="#rate" text="rate" />
   </children>
</AnchorPane>
