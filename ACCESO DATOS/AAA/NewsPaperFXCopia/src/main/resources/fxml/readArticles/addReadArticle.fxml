<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.screens.readArticle.ReadArticleController">
    <TableView fx:id="readArticlesTable" layoutX="525.0" layoutY="28.0" prefHeight="422.0" prefWidth="438.0">
      <columns>
          <TableColumn fx:id="idColumn" prefWidth="104.0" text="id" />
          <TableColumn fx:id="idReaderColumn" prefWidth="118.0" text="idReader" />
          <TableColumn fx:id="idArticleColumn" prefWidth="116.0" text="idArticle" />
          <TableColumn fx:id="ratingColumn" prefWidth="99.0" text="rating" />
      </columns>
    </TableView>
   <TextField fx:id="textId" layoutX="14.0" layoutY="28.0" promptText="id" />
   <TextField fx:id="textRating" layoutX="14.0" layoutY="163.0" promptText="rating" />
   <ComboBox fx:id="comboReader" layoutX="14.0" layoutY="78.0" prefWidth="150.0" promptText="Reader" />
   <ComboBox fx:id="comboArticle" layoutX="14.0" layoutY="122.0" prefWidth="150.0" promptText="Article" />
   <Button layoutX="14.0" layoutY="208.0" mnemonicParsing="false" onAction="#addReadArticle" text="Add Read Article" />
</AnchorPane>
